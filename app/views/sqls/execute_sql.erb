<h1> RESULTS </h1>
<% unless @result.nil? %>
	<h4 class='main-title'><%= @result.length %> results </h4>
	<table class='table table-bordered'>
	<tr>
		<% @result[0].each do |k, v|  %>
			<td><b> <%= k %> </b></td>
		<% end %>
	</tr>

	<% @result.each do |r| %>
	<tr>
		<% r.each do |k, v|  %>
			<td> <%= v %> </td>
		<% end %>
	</tr>
	<% end %>
	</table>
<% end %>	